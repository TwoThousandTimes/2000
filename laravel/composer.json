{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"repositories": [
	    {
		"type": "vcs",
		"url": "https://github.com/Zizaco/ardent.git"
	    }
	],
	"require": {
		"patchwork/utf8": "1.1.*",
		"laravel/framework": "4.1.x",
		"zizaco/entrust": "dev-master",
		"zizaco/confide": "1.0.x",
		"way/guard-laravel": "dev-master",
		"way/generators": "dev-master",
		"solarium/solarium": "dev-develop",
		"pda/pheanstalk": "dev-master",		
		"jenssegers/mongodb": "*",
		"mews/captcha": "dev-master",
		"thomaswelton/laravel-rackspace-opencloud": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/helpers",
			"app/controllers/rest",
			"app/controllers/admin",
			"app/controllers/moderator",
			"app/controllers",
			"app/applogic",
			"app/applogic/facades",
			"app/applogic/providers",
			"app/lib",
			"app/repositories",
			"app/repositories/Post",
			"app/repositories/Category",
			"app/models",
			"app/queue",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
